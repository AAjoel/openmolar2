#
# configuration file to allow openmolar-server debian package 
# use debconf to create an ssl cert.
# the cert is needed so that connection to the 230 server
# is over ssl encryption.
#

RANDFILE                = /dev/urandom

[ req ]
default_bits            = 2048
default_keyfile         = privkey.pem
distinguished_name      = req_distinguished_name
prompt                  = no
policy			= policy_anything

[ req_distinguished_name ]
commonName		= @HostName@
